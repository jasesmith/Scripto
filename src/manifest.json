{
  "manifest_version": 2,
  "name": "Scripto",
  "description": "Write (and actually use) notes",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html",
    "default_title": "Your notes in your face!"
  },
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
